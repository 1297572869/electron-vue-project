{
  "name": "my-electron",
  "version": "0.1.0",
  "description": "My Superb Vue Project",
  "main": "client/index.js",
  "scripts": {
    "test": "",
    "build": "webpack --config build/webpack.prod.js",
    "dev": "node build/server.js",
    "app": "cross-env NODE_ENV=development electron app/",
    "lint": "xo **/client/**/*.{vue,js} **/app/**/*.js !node_modules/**",
    "packageDarwin": "electron-packager . 'TEST' --platform=darwin --arch=x64 --icon=hosts.icns --out=./package --asar --app-version=2.0.3 --ignore=\"(build|client|static|.gitignore|LICENSE|README.md|.editorconfig|.eslintrc|node_modules)\"",
    "packageWin": "electron-packager . 'TEST' --platform=win32 --arch=x64 --icon=hosts.ico --out=./package --asar --app-version=2.0.3 --ignore=\"(build|client|static|.gitignore|LICENSE|README.md|.editorconfig|.eslintrc|node_modules)\"",
    "packageLinux": "electron-packager . 'TEST' --platform=linux --arch=x64 --out=./package --asar --app-version=2.0.3 --ignore=\"(build|client|static|.gitignore|LICENSE|README.md|.editorconfig|.eslintrc|node_modules)\"",
    "package": "npm run packageDarwin && npm run packageWin && npm run packageLinux"
  },
  "author": "Howard.Zuo",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-component": "^0.9.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.2",
    "electron-packager": "^8.7.0",
    "electron-prebuilt": "^1.4.13",
