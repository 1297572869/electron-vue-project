    "postcss-nested": "^2.0.2",
    "postcss-simple-vars": "^4.0.0",
    "style-loader": "^0.18.1",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.1.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "element-ui": "^1.3.4",
    "hostile": "^1.3.0",
