{
  "name": "my-electron",
  "version": "0.1.0",
  "description": "My Superb Vue Project",
  "main": "app/index.js",
  "scripts": {
    "test": "",
    "build": "rimraf app/dist && webpack --config build/webpack.prod.js",
    "dev": "node build/server.js",
    "app": "cross-env NODE_ENV=development electron app/",
    "lint": "xo **/client/**/*.{vue,js} **/app/**/*.js !node_modules/**",
    "packageDarwin": "electron-packager . TEST --platform=darwin --arch=x64 --overwrite --icon=./hosts.icns --out=./package --asar --ignore=\"(build|client|static|.gitignore|LICENSE|README.md|.editorconfig|.eslintrc|node_modules)\"",
    "packageWin": "rimraf package && electron-packager . TEST --platform=win32 --arch=x64 --overwrite --icon=./hosts.ico --out=./package --ignore=\"(build|client$|static|.gitignore|LICENSE|README.md|.editorconfig|.eslintrc|node_modules)\"",
    "packageLinux": "electron-packager . TEST --platform=linux --arch=x64 --overwrite --out=/package --asar --ignore=\"(build|client|static|.gitignore|LICENSE|README.md|.editorconfig|.eslintrc|node_modules)\"",
    "package": "npm run packageDarwin && npm run packageWin && npm run packageLinux"
  },
  "license": "MIT",
  "author": "xiaobinwu <xiaobin_wu@yahoo.com>",
  "license": "MIT",
  "xo": {
    "extends": "./.eslintrc",
    "esnext": true,
    "envs": [
      "browser"
    ]
  },
  "babel": {
    "presets": [
      [
        "vue-app",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-nested": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 8"
  ],
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "promise-polyfill": "^6.0.2",
    "vue": "^2.2.0",
    "vue-router": "^2.0.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.4",
    "babel-preset-vue-app": "^1.1.1",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "electron": "^1.4.2",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.7.1",
    "eslint": "^3.19.0",
    "eslint-config-vue": "latest",
    "eslint-plugin-vue": "latest",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "html-webpack-plugin": "^2.22.0",
    "offline-plugin": "^4.5.3",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "vue-loader": "^11.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.4",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2",
    "xo": "^0.18.0"
  }
}